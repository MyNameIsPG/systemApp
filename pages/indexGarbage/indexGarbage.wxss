.contern {
    position: absolute;
    z-index: 999;
    background: rgba(255,255,255,0.8);
    left: 30rpx;
    right: 30rpx;
    top: 143rpx;
    border-radius: 30rpx;
    padding: 20rpx;
}

.table {
    border: 2px solid #27ccaa;
    border-right: 0px;
    border-bottom: 0px;
    width: 100%;
}

.tr {
    width: 100%;
    overflow: hidden;
}

.th {
    color: #333333;
    font-size: 24rpx;
    font-weight: 600;
    padding: 10rpx 0;
    float: left;
    width: 33%;
    text-align: center;
    display: inline-block;
}

.td {
    vertical-align: middle;
    text-align: center;
    color: #333333;
    font-size: 20rpx;
    display: table-cell;
    float: left;
    width: 33%;
}




